{
  "name": "reactplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=7",
    "npm": ">=5",
    "yarn": ">=0.27"
  },
  "private": true,
  "standard": {
    "globals": [
      ""
    ]
  },
  "scripts": {
    "build": "npm-run-all --parallel clean compile",
    "build:watch": "nodemon -w src --exec yarn build",
    "clean": "rimraf public/*.{js,css,map}",
    "compile": "cross-env NODE_ENV=production webpack --progress --config config/webpack",
    "flow": "flow",
    "lint": "standard src",
    "start": "cross-env NODE_ENV=development PORT=8001 CONFIG_DIR=./.storybook start-storybook",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "ejs-loader": "^0.3.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "flow-bin": "^0.50.0",
    "flow-typed": "^2.1.5",
    "html-webpack-plugin": "^2.29.0",
    "ignore-loader": "^0.1.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.7",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-chain": "^3.3.0",
    "webpack-stats-plugin": "^0.1.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "recompose": "^0.24.0"
  }
}

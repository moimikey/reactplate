{
  "name": "reactplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "private": true,
  "scripts": {
    "build": "npm-run-all -s cleanup lint compile size",
    "build:watch": "nodemon -w src --exec npm-run-all build",
    "cleanup": "rimraf \"public/!(vendor*).{js,css,map,md,json}\"",
    "lint": "standard --verbose \"src/**/*.js\" | snazzy",
    "lint:fix": "standard --verbose --fix \"src/**/*.js\" | snazzy",
    "size": "dist-size -e public",
    "start": "nodemon -w src/config --exec start-storybook -p 8001 -c src/config/storybook",
    "start:server": "nodemon --watch src/server --exec babel-node src/server",
    "test": "jest --coverage",
    "test:staged": "jest -o --coverage",
    "test:watch": "jest --notify --watch"
  },
  "jest": {
    "verbose": true,
    "testRegex": "(.*\\.spec)\\.js$",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "setupTestFrameworkScriptFile": "./.jest.js"
  },
  "standard": {
    "globals": [
      "shallow",
      "render",
      "mount",
      "toJson"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype",
      "react"
    ],
    "env": [
      "browser",
      "jest",
      "es6",
      "shared-node-browser"
    ]
  },
  "devDependencies": {
    "@storybook/cli": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.3.2",
    "compression": "^1.7.2",
    "crossenv": "^0.0.2-security",
    "dist-size": "^0.4.0",
    "dotenv": "^5.0.1",
    "has-dom": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "koa": "^2.5.0",
    "koa-logger": "^3.2.0",
    "koa2-static-files": "^1.0.4",
    "nodemon": "^1.17.2",
    "prompts": "^0.1.8",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.1",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "wait-on": "^2.1.0",
    "webpack": "^4.2.0",
    "winston": "^2.4.1"
  },
  "dependencies": {
    "core-js": "^2.5.3",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
